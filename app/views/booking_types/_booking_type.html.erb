<div id="<%= "#{dom_id booking_type}_wrapper" %>" class="col-12 col-md-4 col-xl-3">
  <%= turbo_frame_tag booking_type do %>
    <%= link_to booking_type, class: "text-decoration-none", data: {turbo: false}  do %>
      <div class="card bg-c-<%= %w[blue green yellow pink].sample %> order-card">
        <div class="card-block">
          <div class="text-right">User: <span class="f-right"><%= booking_type.user_id %></span></div>
          <div class="text-right">Cost:<span class="f-right"><%= booking_type.cost_id %></span></div>
          <div class="text-right">Duration:<span class="f-right"><%= booking_type.duration %></span></div>
          <div class="text-right">Name:<span class="f-right"><%= booking_type.name %></span></div>
        </div>
      </div>
    <% end %>
  <% end %>
</div>
